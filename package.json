{
  "name": "wikibase-vuejs-components",
  "version": "0.1.0",
  "description": "A library for vuejs components used in Wikibase projects. Inspired by OOUI",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wmde/wikibase-vuejs-components.git"
  },
  "bugs": {
    "url": "https://github.com/wmde/wikibase-vuejs-components/issues"
  },
  "homepage": "https://github.com/wmde/wikibase-vuejs-components#readme",
  "keywords": [
    "vuejs",
    "OOUI",
    "Mediawiki",
    "Wikibase",
    "components"
  ],
  "license": "GPL-2.0+",
  "main": "./dist/main.umd.js",
  "module": "./dist/main.common.js",
  "types": "./dist/index.d.ts",
  "files": [
    "src/",
    "dist/"
  ],
  "scripts": {
    "build": "NODE_ENV=production vue-cli-service build --no-clean --target lib --name main src/index.ts",
    "fix": "vue-cli-service lint --max-warnings 0 .",
    "test": "npm-run-all test:*",
    "test:lint": "vue-cli-service lint --max-warnings 0 --no-fix . && stylelint --syntax scss 'src/**/*.(vue|scss)'",
    "test:unit": "vue-cli-service test:unit"
  },
  "dependencies": {},
  "peer-depenecies": {
    "vue": "^2.6.10"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.6.0",
    "@typescript-eslint/parser": "^2.6.0",
    "@vue/cli-plugin-babel": "^4.0.5",
    "@vue/cli-plugin-eslint": "^4.0.5",
    "@vue/cli-plugin-unit-jest": "^4.0.5",
    "@vue/cli-service": "^4.0.5",
    "@vue/test-utils": "^1.0.0-beta.29",
    "eslint": "^6.6.0",
    "eslint-config-wikimedia": "^0.15.0",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-vue": "^5.2.3",
    "npm-run-all": "^4.1.5",
    "stylelint": "^11.1.1",
    "stylelint-config-wikimedia": "^0.7.0",
    "stylelint-processor-html": "^1.0.0",
    "stylelint-scss": "^3.12.0",
    "ts-jest": "^24.1.0",
    "typescript": "^3.6.4",
    "vue-class-component": "^7.1.0",
    "vue-cli-plugin-typescript": "0.0.1",
    "vue-property-decorator": "^8.3.0",
    "vue-template-compiler": "^2.6.10"
  }
}
